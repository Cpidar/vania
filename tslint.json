{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "semicolon": false,
    "trailing-comma": false,
    "quotemark": [
      true,
      "single"
    ],
    "indent": [
      true
    ],
    "interface-name": [
      false
    ],
    "arrow-parens": false,
    // Pending fix for shorthand property names.
    "object-literal-sort-keys": false
  },
  "rulesDirectory": []
}
